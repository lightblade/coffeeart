#!/usr/bin/env coffee

vm = require 'vm'
url = require 'url'
path = require 'path'

coffeeart = require '../lib/coffeeart'

process.stdin.setEncoding 'utf8'
process.stdin.on 'data', (chunk) -> buf += chunk
process.stdin.on 'end', -> process.stdout.write coffeeart.compile buf

process.stdin.resume()

buf = ''
